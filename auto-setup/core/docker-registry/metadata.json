{
    "name": "docker-registry",
    "namespace": "docker-registry",
    "installation_type": "helm",
    "installation_group_folder": "core",
    "environment_variables": {
        "imageTag": "2.7.1"
    },
    "retry": "true",
    "helm_params": {
        "repository_url": "https://helm.twun.io",
        "repository_name": "twuni/docker-registry",
        "helm_version": "v1.5.4",
        "set_key_values": [
            "image.tag={{imageTag}}",
            "installCRDs=true"
        ]
    },
    "urls": [
        {
            "url": "https://{{componentName}}.{{baseDomain}}",
            "healthchecks": {
                "liveliness": {
                    "http_path": "/",
                    "http_auth_required": false,
                    "expected_http_response_code": "200",
                    "expected_http_response_string": "",
                    "expected_json_response": {
                        "json_path": ".status",
                        "json_value": "ok"
                    },
                    "health_shell_check_command": "",
                    "expected_shell_check_command_response": ""
                },
                "readiness": {
                    "http_path": "/",
                    "http_auth_required": false,
                    "expected_http_response_code": "200",
                    "expected_http_response_string": "",
                    "expected_json_response": {
                        "json_path": ".status",
                        "json_value": "ok"
                    }
                }
            }
        }
    ],
    "install_scripts": [],
    "Description": "Docker Registry",
    "shortcut_text": "Docker Registry",
    "shortcut_icon": "docker-registry.png",
    "pre_install_scripts": [],
    "post_install_scripts": []
}
