<com.cloudbees.jenkins.plugins.awscredentials.AWSCredentialsImpl plugin="aws-credentials@1.29">
    <scope>GLOBAL</scope>
    <id>AWS_TERRAFORM_ACCESS</id>
    <description>AWS credentials for deploying KX.AS.CODE AMI images with Hashicorp Terraform</description>
    <accessKey>{{terraform_aws_terraform_access_key}}</accessKey>
    <secretKey>{{terraform_aws_terraform_access_secret}}</secretKey>
    <iamRoleArn></iamRoleArn>
    <iamMfaSerialNumber></iamMfaSerialNumber>
</com.cloudbees.jenkins.plugins.awscredentials.AWSCredentialsImpl>
