<?xml version='1.1' encoding='UTF-8'?>
<flow-definition plugin="workflow-job@2.40">
  <actions>
    <org.jenkinsci.plugins.pipeline.modeldefinition.actions.DeclarativeJobAction plugin="pipeline-model-definition@1.7.2"/>
    <org.jenkinsci.plugins.pipeline.modeldefinition.actions.DeclarativeJobPropertyTrackerAction plugin="pipeline-model-definition@1.7.2">
      <jobProperties>
        <string>org.jenkinsci.plugins.workflow.job.properties.DisableConcurrentBuildsJobProperty</string>
        <string>jenkins.model.BuildDiscarderProperty</string>
      </jobProperties>
      <triggers/>
      <parameters>
        <string>availability_zone</string>
        <string>ssh_username</string>
        <string>vpc_id</string>
        <string>shutdown_behavior</string>
        <string>kx_vm_user</string>
        <string>associate_public_ip_address</string>
        <string>security_group_id</string>
        <string>ssh_interface</string>
        <string>git_docs_branch</string>
        <string>git_techradar_branch</string>
        <string>kx_version</string>
        <string>base_image_ssh_user</string>
        <string>kx_hostname</string>
        <string>vpc_subnet_id</string>
        <string>git_source_branch</string>
        <string>ami_groups</string>
        <string>kx_vm_password</string>
        <string>git_repo_url</string>
        <string>kx_compute_engine_build</string>
        <string>source_ami</string>
        <string>kx_domain</string>
        <string>vpc_region</string>
        <string>instance_type</string>
      </parameters>
      <options>
        <string>skipDefaultCheckout</string>
      </options>
    </org.jenkinsci.plugins.pipeline.modeldefinition.actions.DeclarativeJobPropertyTrackerAction>
  </actions>
  <description></description>
  <keepDependencies>false</keepDependencies>
  <properties>
    <jenkins.model.BuildDiscarderProperty>
      <strategy class="hudson.tasks.LogRotator">
        <daysToKeep>-1</daysToKeep>
        <numToKeep>10</numToKeep>
        <artifactDaysToKeep>-1</artifactDaysToKeep>
        <artifactNumToKeep>10</artifactNumToKeep>
      </strategy>
    </jenkins.model.BuildDiscarderProperty>
    <org.jenkinsci.plugins.workflow.job.properties.DisableConcurrentBuildsJobProperty/>
    <hudson.model.ParametersDefinitionProperty>
      <parameterDefinitions>
        <hudson.model.StringParameterDefinition>
          <name>git_repo_url</name>
          <description>Source Github repository</description>
          <defaultValue>github.com/Accenture/kx.as.code.git</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>git_source_branch</name>
          <description>Source Github branch to build from and clone inside VM</description>
          <defaultValue>main</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>git_docs_branch</name>
          <description>Docs Github branch to clone</description>
          <defaultValue>main</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>git_techradar_branch</name>
          <description>TechRadar Github branch to clone</description>
          <defaultValue>main</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>kx_version</name>
          <description>KX.AS.CODE Version</description>
          <defaultValue>0.6.7</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>kx_vm_user</name>
          <description>KX.AS.CODE VM user login</description>
          <defaultValue>kx.hero</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>kx_vm_password</name>
          <description>KX.AS.CODE VM user login password</description>
          <defaultValue>L3arnandshare</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>kx_compute_engine_build</name>
          <description>Needs to be true for AWS to avoid &apos;grub&apos; changes</description>
          <defaultValue>true</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>kx_hostname</name>
          <description>KX.AS.CODE main node hostname</description>
          <defaultValue>kx-main</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>kx_domain</name>
          <description>KX.AS.CODE local domain</description>
          <defaultValue>kx-as-code.local</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>base_image_ssh_user</name>
          <description>Default AMI SSH user</description>
          <defaultValue>admin</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>ami_groups</name>
          <description>AWS user group that can launch AMI</description>
          <defaultValue>kxascode</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>vpc_region</name>
          <description>VPC region, eg. us-east-2</description>
          <defaultValue>us-east-2</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>vpc_id</name>
          <description>VPC Id</description>
          <defaultValue>vpc-051354ba4e689b0b4</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>vpc_subnet_id</name>
          <description>VPC Subnet Id</description>
          <defaultValue>subnet-038d9eabbbb629e5c</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>availability_zone</name>
          <description>VPC Availability zone, eg. us-east-2c</description>
          <defaultValue>us-east-2c</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>associate_public_ip_address</name>
          <description>Assign public IP. Should be true or false</description>
          <defaultValue>true</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>source_ami</name>
          <description>Source AMI set to Debian Buster 10.9</description>
          <defaultValue>ami-0f42acddbf04bd1b6</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>security_group_id</name>
          <description>VPC Security Group Id</description>
          <defaultValue>sg-0772da3ed04fe2677</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>instance_type</name>
          <description>Instance type, eg t3.small</description>
          <defaultValue>t3.small</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>shutdown_behavior</name>
          <description>Stop or Terminate instance on failure</description>
          <defaultValue>terminate</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>ssh_username</name>
          <description>SSH user used during packer build process</description>
          <defaultValue>admin</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>ssh_interface</name>
          <description>Options are private_ip or public_ip</description>
          <defaultValue>public_ip</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
      </parameterDefinitions>
    </hudson.model.ParametersDefinitionProperty>
  </properties>
  <definition class="org.jenkinsci.plugins.workflow.cps.CpsScmFlowDefinition" plugin="workflow-cps@2.87">
    <scm class="hudson.plugins.git.GitSCM" plugin="git@4.5.1">
      <configVersion>2</configVersion>
      <userRemoteConfigs>
        <hudson.plugins.git.UserRemoteConfig>
          <url>https://github.com/Accenture/kx.as.code.git</url>
          <credentialsId>GITHUB_KX.AS.CODE</credentialsId>
        </hudson.plugins.git.UserRemoteConfig>
      </userRemoteConfigs>
      <branches>
        <hudson.plugins.git.BranchSpec>
          <name>main</name>
        </hudson.plugins.git.BranchSpec>
      </branches>
      <doGenerateSubmoduleConfigurations>false</doGenerateSubmoduleConfigurations>
      <submoduleCfg class="list"/>
      <extensions/>
    </scm>
    <scriptPath>base-vm/build/jenkins/kx.as.code-main-aws.Jenkinsfile</scriptPath>
    <lightweight>true</lightweight>
  </definition>
  <triggers/>
  <disabled>false</disabled>
</flow-definition>