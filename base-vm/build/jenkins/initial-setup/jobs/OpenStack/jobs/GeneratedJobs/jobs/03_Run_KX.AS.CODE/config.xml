<?xml version='1.1' encoding='UTF-8'?>
<flow-definition plugin="workflow-job@2.40">
  <actions>
    <org.jenkinsci.plugins.pipeline.modeldefinition.actions.DeclarativeJobAction plugin="pipeline-model-definition@1.7.2"/>
    <org.jenkinsci.plugins.pipeline.modeldefinition.actions.DeclarativeJobPropertyTrackerAction plugin="pipeline-model-definition@1.7.2">
      <jobProperties>
        <string>org.jenkinsci.plugins.workflow.job.properties.DisableConcurrentBuildsJobProperty</string>
        <string>jenkins.model.BuildDiscarderProperty</string>
      </jobProperties>
      <triggers/>
      <parameters>
        <string>openstack_project_domain_name</string>
        <string>openstack_kx_worker_image_id</string>
        <string>openstack_project_name</string>
        <string>openstack_user_domain_name</string>
        <string>git_source_branch</string>
        <string>openstack_kx_main_image_id</string>
        <string>openstack_region_name</string>
        <string>terraform_action</string>
        <string>openstack_identity_api_version</string>
        <string>git_repo_url</string>
        <string>openstack_auth_url</string>
        <string>openstack_external_network_id</string>
      </parameters>
      <options>
        <string>skipDefaultCheckout</string>
      </options>
    </org.jenkinsci.plugins.pipeline.modeldefinition.actions.DeclarativeJobPropertyTrackerAction>
  </actions>
  <description></description>
  <keepDependencies>false</keepDependencies>
  <properties>
    <jenkins.model.BuildDiscarderProperty>
      <strategy class="hudson.tasks.LogRotator">
        <daysToKeep>-1</daysToKeep>
        <numToKeep>5</numToKeep>
        <artifactDaysToKeep>-1</artifactDaysToKeep>
        <artifactNumToKeep>5</artifactNumToKeep>
      </strategy>
    </jenkins.model.BuildDiscarderProperty>
    <org.jenkinsci.plugins.workflow.job.properties.DisableConcurrentBuildsJobProperty/>
    <hudson.model.ParametersDefinitionProperty>
      <parameterDefinitions>
        <hudson.model.ChoiceParameterDefinition>
          <name>terraform_action</name>
          <description>Selection Terraform action to execute</description>
          <choices class="java.util.Arrays$ArrayList">
            <a class="string-array">
              <string>init</string>
              <string>plan</string>
              <string>apply</string>
              <string>destroy</string>
            </a>
          </choices>
        </hudson.model.ChoiceParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>git_repo_url</name>
          <description>Source Github repository</description>
          <defaultValue>{{git_repo_url}}</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>git_source_branch</name>
          <description>Source Github branch to build from and clone inside VM</description>
          <defaultValue>{{git_source_branch}}</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>openstack_external_network_id</name>
          <description>OpenStack External Network ID</description>
          <defaultValue>{{openstack_external_network_id}}</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>openstack_kx_main_image_id</name>
          <description>OpenStack KX-Main image ID</description>
          <defaultValue>{{openstack_kx_main_image_id}}</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>openstack_kx_worker_image_id</name>
          <description>OpenStack KX-Worker image ID</description>
          <defaultValue>{{openstack_kx_worker_image_id}}</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>openstack_region_name</name>
          <description>OpenStack Region Name</description>
          <defaultValue>{{openstack_region_name}}</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>openstack_auth_url</name>
          <description>OpenStack Auth URL</description>
          <defaultValue>{{openstack_auth_url}}</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>openstack_project_name</name>
          <description>OpenStack Project Name</description>
          <defaultValue>{{openstack_project_name}}</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>openstack_user_domain_name</name>
          <description>OpenStack User Domain Name</description>
          <defaultValue>{{openstack_user_domain_name}}</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>openstack_project_domain_name</name>
          <description>OpenStack Project Domain Name</description>
          <defaultValue>{{openstack_project_domain_name}}</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>openstack_identity_api_version</name>
          <description>OpenStack Identity API Version</description>
          <defaultValue>3</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
      </parameterDefinitions>
    </hudson.model.ParametersDefinitionProperty>
  </properties>
  <definition class="org.jenkinsci.plugins.workflow.cps.CpsScmFlowDefinition" plugin="workflow-cps@2.87">
    <scm class="hudson.plugins.git.GitSCM" plugin="git@4.5.1">
      <configVersion>2</configVersion>
      <userRemoteConfigs>
        <hudson.plugins.git.UserRemoteConfig>
          <url>{{git_repo_url}}</url>
          <credentialsId>GITHUB_KX.AS.CODE</credentialsId>
        </hudson.plugins.git.UserRemoteConfig>
      </userRemoteConfigs>
      <branches>
        <hudson.plugins.git.BranchSpec>
          <name>{{git_source_branch}}</name>
        </hudson.plugins.git.BranchSpec>
      </branches>
      <doGenerateSubmoduleConfigurations>false</doGenerateSubmoduleConfigurations>
      <submoduleCfg class="list"/>
      <extensions/>
    </scm>
    <scriptPath>base-vm/build/jenkins/pipelines/deploy-kx.as.code-openstack.Jenkinsfile</scriptPath>
    <lightweight>true</lightweight>
  </definition>
  <triggers/>
  <disabled>false</disabled>
</flow-definition>