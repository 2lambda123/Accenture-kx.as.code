<?xml version='1.1' encoding='UTF-8'?>
<flow-definition plugin="workflow-job@2.40">
  <actions>
    <org.jenkinsci.plugins.pipeline.modeldefinition.actions.DeclarativeJobAction plugin="pipeline-model-definition@1.7.2"/>
    <org.jenkinsci.plugins.pipeline.modeldefinition.actions.DeclarativeJobPropertyTrackerAction plugin="pipeline-model-definition@1.7.2">
      <jobProperties>
        <string>org.jenkinsci.plugins.workflow.job.properties.DisableConcurrentBuildsJobProperty</string>
        <string>jenkins.model.BuildDiscarderProperty</string>
      </jobProperties>
      <triggers/>
      <parameters>
        <string>openstack_flavor</string>
        <string>openstack_security_groups</string>
        <string>openstack_ssh_username</string>
        <string>kx_vm_user</string>
        <string>openstack_region</string>
        <string>kx_version</string>
        <string>base_image_ssh_user</string>
        <string>kx_worker_hostname</string>
        <string>openstack_networks</string>
        <string>openstack_floating_ip_network</string>
        <string>openstack_source_image</string>
        <string>git_source_branch</string>
        <string>kx_vm_password</string>
        <string>git_repo_url</string>
        <string>openstack_compute_engine_build</string>
        <string>kx_domain</string>
      </parameters>
      <options>
        <string>skipDefaultCheckout</string>
      </options>
    </org.jenkinsci.plugins.pipeline.modeldefinition.actions.DeclarativeJobPropertyTrackerAction>
  </actions>
  <description></description>
  <keepDependencies>false</keepDependencies>
  <properties>
    <jenkins.model.BuildDiscarderProperty>
      <strategy class="hudson.tasks.LogRotator">
        <daysToKeep>-1</daysToKeep>
        <numToKeep>10</numToKeep>
        <artifactDaysToKeep>-1</artifactDaysToKeep>
        <artifactNumToKeep>10</artifactNumToKeep>
      </strategy>
    </jenkins.model.BuildDiscarderProperty>
    <org.jenkinsci.plugins.workflow.job.properties.DisableConcurrentBuildsJobProperty/>
    <hudson.model.ParametersDefinitionProperty>
      <parameterDefinitions>
        <hudson.model.StringParameterDefinition>
          <name>git_repo_url</name>
          <description>Source Github repository</description>
          <defaultValue>{{git_repo_url}}</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>git_source_branch</name>
          <description>Source Github branch to build from and clone inside VM</description>
          <defaultValue>{{git_source_branch}}</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>kx_version</name>
          <description>KX.AS.CODE Version</description>
          <defaultValue>{{kx_version}}</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>kx_vm_user</name>
          <description>KX.AS.CODE VM user login</description>
          <defaultValue>{{kx_vm_user}}</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>kx_vm_password</name>
          <description>KX.AS.CODE VM user login password</description>
          <defaultValue>{{kx_vm_password}}</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>openstack_compute_engine_build</name>
          <description>Should be true for OpenStack</description>
          <defaultValue>{{openstack_compute_engine_build}}</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>kx_worker_hostname</name>
          <description>KX.AS.CODE worker node hostname</description>
          <defaultValue>{{kx_worker_hostname}}</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>kx_domain</name>
          <description>KX.AS.CODE local domain</description>
          <defaultValue>{{kx_domain}}</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>openstack_ssh_username</name>
          <description>SSH user used during packer build process</description>
          <defaultValue>{{openstack_ssh_username}}</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>openstack_region</name>
          <description>OpenStack Region</description>
          <defaultValue>{{openstack_region}}</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>openstack_networks</name>
          <description>OpenStack Network</description>
          <defaultValue>{{openstack_networks}}</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>openstack_floating_ip_network</name>
          <description>OpenStack Floating IP Network</description>
          <defaultValue>{{openstack_floating_ip_network}}</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>openstack_source_image</name>
          <description>OpenStack Source Image</description>
          <defaultValue>{{openstack_source_image}}</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>openstack_flavor</name>
          <description>OpenStack Flavor</description>
          <defaultValue>{{openstack_flavor}}</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>openstack_security_groups</name>
          <description>OpenStack Security Groups</description>
          <defaultValue>{{openstack_security_groups}}</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
      </parameterDefinitions>
    </hudson.model.ParametersDefinitionProperty>
  </properties>
  <definition class="org.jenkinsci.plugins.workflow.cps.CpsScmFlowDefinition" plugin="workflow-cps@2.87">
    <scm class="hudson.plugins.git.GitSCM" plugin="git@4.5.1">
      <configVersion>2</configVersion>
      <userRemoteConfigs>
        <hudson.plugins.git.UserRemoteConfig>
          <url>{{git_repo_url}}</url>
          <credentialsId>GITHUB_KX.AS.CODE</credentialsId>
        </hudson.plugins.git.UserRemoteConfig>
      </userRemoteConfigs>
      <branches>
        <hudson.plugins.git.BranchSpec>
          <name>{{git_source_branch}}</name>
        </hudson.plugins.git.BranchSpec>
      </branches>
      <doGenerateSubmoduleConfigurations>false</doGenerateSubmoduleConfigurations>
      <submoduleCfg class="list"/>
      <extensions/>
    </scm>
    <scriptPath>base-vm/build/jenkins/pipelines/build-kx.as.code-worker-openstack.Jenkinsfile</scriptPath>
    <lightweight>true</lightweight>
  </definition>
  <triggers/>
  <disabled>false</disabled>
</flow-definition>