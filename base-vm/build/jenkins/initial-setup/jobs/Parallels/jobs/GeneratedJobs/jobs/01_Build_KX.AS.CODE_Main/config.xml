<?xml version='1.1' encoding='UTF-8'?>
<flow-definition plugin="workflow-job@2.40">
  <actions>
    <org.jenkinsci.plugins.pipeline.modeldefinition.actions.DeclarativeJobAction plugin="pipeline-model-definition@1.7.2"/>
    <org.jenkinsci.plugins.pipeline.modeldefinition.actions.DeclarativeJobPropertyTrackerAction plugin="pipeline-model-definition@1.7.2">
      <jobProperties>
        <string>org.jenkinsci.plugins.workflow.job.properties.DisableConcurrentBuildsJobProperty</string>
        <string>jenkins.model.BuildDiscarderProperty</string>
      </jobProperties>
      <triggers/>
      <parameters>
        <string>vagrant_ssh_username</string>
        <string>git_source_branch</string>
        <string>kx_vm_user</string>
        <string>kx_vm_password</string>
        <string>git_source_url</string>
        <string>git_repo_url</string>
        <string>git_techradar_url</string>
        <string>vagrant_compute_engine_build</string>
        <string>git_docs_branch</string>
        <string>git_techradar_branch</string>
        <string>kx_version_override</string>
        <string>kx_domain</string>
        <string>kx_main_hostname</string>
        <string>shared_workspace</string>
      </parameters>
      <options>
        <string>skipDefaultCheckout</string>
      </options>
    </org.jenkinsci.plugins.pipeline.modeldefinition.actions.DeclarativeJobPropertyTrackerAction>
  </actions>
  <description></description>
  <keepDependencies>false</keepDependencies>
  <properties>
    <jenkins.model.BuildDiscarderProperty>
      <strategy class="hudson.tasks.LogRotator">
        <daysToKeep>-1</daysToKeep>
        <numToKeep>10</numToKeep>
        <artifactDaysToKeep>-1</artifactDaysToKeep>
        <artifactNumToKeep>10</artifactNumToKeep>
      </strategy>
    </jenkins.model.BuildDiscarderProperty>
    <org.jenkinsci.plugins.workflow.job.properties.DisableConcurrentBuildsJobProperty/>
    <hudson.model.ParametersDefinitionProperty>
      <parameterDefinitions>
        <hudson.model.StringParameterDefinition>
          <name>git_source_url</name>
          <description>Source Git repository</description>
          <defaultValue>{{git_source_url}}</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>git_docs_url</name>
          <description>Docs Git repository</description>
          <defaultValue>{{git_docs_url}}</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>git_techradar_url</name>
          <description>TechRadar Git repository</description>
          <defaultValue>{{git_techradar_url}}</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>git_source_branch</name>
          <description>Source Github branch to build from and clone inside VM</description>
          <defaultValue>{{git_source_branch}}</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>git_docs_branch</name>
          <description>Docs Github branch to clone</description>
          <defaultValue>{{git_docs_branch}}</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>git_techradar_branch</name>
          <description>TechRadar Github branch to clone</description>
          <defaultValue>{{git_techradar_branch}}</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>kx_version_override</name>
          <description>KX.AS.CODE Version</description>
          <defaultValue></defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>kx_vm_user</name>
          <description>KX.AS.CODE VM user login</description>
          <defaultValue>{{kx_vm_user}}</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>kx_vm_password</name>
          <description>KX.AS.CODE VM user login password</description>
          <defaultValue>{{kx_vm_password}}</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>vagrant_compute_engine_build</name>
          <description>Should be false for Vagrant based builds</description>
          <defaultValue>{{vagrant_compute_engine_build}}</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>kx_main_hostname</name>
          <description>KX.AS.CODE main node hostname</description>
          <defaultValue>{{kx_main_hostname}}</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>kx_domain</name>
          <description>KX.AS.CODE local domain</description>
          <defaultValue>{{kx_domain}}</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>vagrant_ssh_username</name>
          <description>SSH user used during packer build process</description>
          <defaultValue>{{vagrant_ssh_username}}</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>shared_workspace</name>
          <description>Shared workspace for the KX.AS.CODE jobs</description>
          <defaultValue>{{shared_workspace_directory_path}}</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
      </parameterDefinitions>
    </hudson.model.ParametersDefinitionProperty>
  </properties>
  <definition class="org.jenkinsci.plugins.workflow.cps.CpsScmFlowDefinition" plugin="workflow-cps@2.87">
    <scm class="hudson.plugins.git.GitSCM" plugin="git@4.5.1">
      <configVersion>2</configVersion>
      <userRemoteConfigs>
        <hudson.plugins.git.UserRemoteConfig>
          <url>{{git_source_url}}</url>
          <credentialsId>GIT_KX.AS.CODE_SOURCE</credentialsId>
        </hudson.plugins.git.UserRemoteConfig>
      </userRemoteConfigs>
      <branches>
        <hudson.plugins.git.BranchSpec>
          <name>{{git_source_branch}}</name>
        </hudson.plugins.git.BranchSpec>
      </branches>
      <doGenerateSubmoduleConfigurations>false</doGenerateSubmoduleConfigurations>
      <submoduleCfg class="list"/>
      <extensions/>
    </scm>
    <scriptPath>base-vm/build/jenkins/pipelines/build-kx.as.code-main-parallels.Jenkinsfile</scriptPath>
    <lightweight>true</lightweight>
  </definition>
  <triggers/>
  <disabled>false</disabled>
</flow-definition>
